        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectSysNotaryProcessLogVo">
        select log_id, notary_id, process_step, step_name, action, content, status, operator_id, operator_name, operation_time, operate_time, comment, attachment_url, create_by, create_time, update_by, update_time, remark from sys_notary_process_log
    </sql>

    <select id="selectSysNotaryProcessLogList" parameterType="SysNotaryProcessLog" resultMap="SysNotaryProcessLogResult">
        <include refid="selectSysNotaryProcessLogVo"/>
        <where>  
            <if test="notaryId != null "> and notary_id = #{notaryId}</if>
            <if test="processStep != null  and processStep != ''"> and process_step = #{processStep}</if>
            <if test="stepName != null  and stepName != ''"> and step_name like concat('%', #{stepName}, '%')</if>
            <if test="action != null  and action != ''"> and action = #{action}</if>
            <if test="content != null  and content != ''"> and content like concat('%', #{content}, '%')</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
            <if test="operatorId != null "> and operator_id = #{operatorId}</if>
            <if test="operatorName != null  and operatorName != ''"> and operator_name like concat('%', #{operatorName}, '%')</if>
            <if test="operationTime != null "> and operation_time = #{operationTime}</if>
            <if test="operateTime != null "> and operate_time = #{operateTime}</if>
        </where>
        order by operate_time desc
    </select>
    
    <select id="selectSysNotaryProcessLogByLogId" parameterType="Long" resultMap="SysNotaryProcessLogResult">
        <include refid="selectSysNotaryProcessLogVo"/>
        where log_id = #{logId}
    </select>
        
    <insert id="insertSysNotaryProcessLog" parameterType="SysNotaryProcessLog" useGeneratedKeys="true" keyProperty="logId">
        insert into sys_notary_process_log
        <trim prefix="(" suffix=")" suffixOverrides=",">
