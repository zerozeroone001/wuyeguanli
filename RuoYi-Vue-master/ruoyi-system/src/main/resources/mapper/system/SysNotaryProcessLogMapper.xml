    <delete id="deleteSysNotaryProcessLogByLogId" parameterType="Long">
        delete from sys_notary_process_log where log_id = #{logId}
    </delete>

    <delete id="deleteSysNotaryProcessLogByLogIds" parameterType="String">
        delete from sys_notary_process_log where log_id in 
        <foreach item="logId" collection="array" open="(" separator="," close=")">
            #{logId}
        </foreach>
    </delete>
