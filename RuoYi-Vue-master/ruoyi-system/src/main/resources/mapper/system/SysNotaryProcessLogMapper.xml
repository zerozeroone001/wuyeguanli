<mapper namespace="com.ruoyi.system.mapper.SysNotaryProcessLogMapper">
    
    <resultMap type="SysNotaryProcessLog" id="SysNotaryProcessLogResult">
        <result property="logId"    column="log_id"    />
        <result property="notaryId"    column="notary_id"    />
        <result property="processStep"    column="process_step"    />
        <result property="stepName"    column="step_name"    />
        <result property="action"    column="action"    />
        <result property="content"    column="content"    />
        <result property="status"    column="status"    />
        <result property="operatorId"    column="operator_id"    />
