<mapper namespace="com.ruoyi.system.mapper.SysNotaryTypeConfigMapper">
    
    <resultMap type="SysNotaryTypeConfig" id="SysNotaryTypeConfigResult">
        <result property="configId"    column="config_id"    />
        <result property="typeCode"    column="type_code"    />
        <result property="typeName"    column="type_name"    />
        <result property="description"    column="description"    />
        <result property="baseFee"    column="base_fee"    />
        <result property="urgentFee"    column="urgent_fee"    />
