    <sql id="selectSysNotaryTypeConfigVo">
        select config_id, type_code, type_name, description, base_fee, urgent_fee, processing_days, urgent_days, required_documents, optional_documents, status, sort_order, create_by, create_time, update_by, update_time, remark from sys_notary_type_config
    </sql>

    <select id="selectSysNotaryTypeConfigList" parameterType="SysNotaryTypeConfig" resultMap="SysNotaryTypeConfigResult">
        <include refid="selectSysNotaryTypeConfigVo"/>
        <where>  
            <if test="typeCode != null  and typeCode != ''"> and type_code = #{typeCode}</if>
            <if test="typeName != null  and typeName != ''"> and type_name like concat('%', #{typeName}, '%')</if>
            <if test="description != null  and description != ''"> and description = #{description}</if>
            <if test="baseFee != null "> and base_fee = #{baseFee}</if>
            <if test="urgentFee != null "> and urgent_fee = #{urgentFee}</if>
            <if test="processingDays != null "> and processing_days = #{processingDays}</if>
            <if test="urgentDays != null "> and urgent_days = #{urgentDays}</if>
            <if test="requiredDocuments != null  and requiredDocuments != ''"> and required_documents = #{requiredDocuments}</if>
            <if test="optionalDocuments != null  and optionalDocuments != ''"> and optional_documents = #{optionalDocuments}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
            <if test="sortOrder != null "> and sort_order = #{sortOrder}</if>
        </where>
        order by sort_order asc, create_time desc
    </select>
    
    <select id="selectSysNotaryTypeConfigByConfigId" parameterType="Long" resultMap="SysNotaryTypeConfigResult">
        <include refid="selectSysNotaryTypeConfigVo"/>
        where config_id = #{configId}
    </select>
        
    <insert id="insertSysNotaryTypeConfig" parameterType="SysNotaryTypeConfig" useGeneratedKeys="true" keyProperty="configId">
