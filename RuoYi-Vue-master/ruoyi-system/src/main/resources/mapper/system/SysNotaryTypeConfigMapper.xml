            <if test="typeName != null and typeName != ''">type_name = #{typeName},</if>
            <if test="description != null">description = #{description},</if>
            <if test="baseFee != null">base_fee = #{baseFee},</if>
            <if test="urgentFee != null">urgent_fee = #{urgentFee},</if>
            <if test="processingDays != null">processing_days = #{processingDays},</if>
            <if test="urgentDays != null">urgent_days = #{urgentDays},</if>
            <if test="requiredDocuments != null">required_documents = #{requiredDocuments},</if>
            <if test="optionalDocuments != null">optional_documents = #{optionalDocuments},</if>
            <if test="status != null">status = #{status},</if>
            <if test="sortOrder != null">sort_order = #{sortOrder},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
