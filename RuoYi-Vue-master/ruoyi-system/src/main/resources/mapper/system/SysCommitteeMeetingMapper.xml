<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.SysCommitteeMeetingMapper">
    
    <resultMap type="SysCommitteeMeeting" id="SysCommitteeMeetingResult">
        <result property="meetingId"    column="meeting_id"    />
        <result property="title"    column="title"    />
        <result property="meetingTime"    column="meeting_time"    />
        <result property="location"    column="location"    />
        <result property="noticeContent"    column="notice_content"    />
        <result property="recordFileUrl"    column="record_file_url"    />
        <result property="resolutionContent"    column="resolution_content"    />
        <result property="status"    column="status"    />
        <result property="delFlag"    column="del_flag"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectSysCommitteeMeetingVo">
        select meeting_id, title, meeting_time, location, notice_content, record_file_url, resolution_content, status, del_flag, create_by, create_time, update_by, update_time, remark from sys_committee_meeting
    </sql>

    <select id="selectSysCommitteeMeetingList" parameterType="SysCommitteeMeeting" resultMap="SysCommitteeMeetingResult">
        <include refid="selectSysCommitteeMeetingVo"/>
        <where>  
            <if test="title != null  and title != ''"> and title = #{title}</if>
            <if test="meetingTime != null "> and meeting_time = #{meetingTime}</if>
            <if test="location != null  and location != ''"> and location = #{location}</if>
            <if test="noticeContent != null  and noticeContent != ''"> and notice_content = #{noticeContent}</if>
            <if test="recordFileUrl != null  and recordFileUrl != ''"> and record_file_url = #{recordFileUrl}</if>
            <if test="resolutionContent != null  and resolutionContent != ''"> and resolution_content = #{resolutionContent}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
        </where>
    </select>
    
    <select id="selectSysCommitteeMeetingByMeetingId" parameterType="Long" resultMap="SysCommitteeMeetingResult">
        <include refid="selectSysCommitteeMeetingVo"/>
        where meeting_id = #{meetingId}
    </select>

    <insert id="insertSysCommitteeMeeting" parameterType="SysCommitteeMeeting" useGeneratedKeys="true" keyProperty="meetingId">
        insert into sys_committee_meeting
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="title != null and title != ''">title,</if>
            <if test="meetingTime != null">meeting_time,</if>
            <if test="location != null">location,</if>
            <if test="noticeContent != null">notice_content,</if>
            <if test="recordFileUrl != null">record_file_url,</if>
            <if test="resolutionContent != null">resolution_content,</if>
            <if test="status != null">status,</if>
            <if test="delFlag != null">del_flag,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="title != null and title != ''">#{title},</if>
            <if test="meetingTime != null">#{meetingTime},</if>
            <if test="location != null">#{location},</if>
            <if test="noticeContent != null">#{noticeContent},</if>
            <if test="recordFileUrl != null">#{recordFileUrl},</if>
            <if test="resolutionContent != null">#{resolutionContent},</if>
            <if test="status != null">#{status},</if>
            <if test="delFlag != null">#{delFlag},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateSysCommitteeMeeting" parameterType="SysCommitteeMeeting">
        update sys_committee_meeting
        <trim prefix="SET" suffixOverrides=",">
            <if test="title != null and title != ''">title = #{title},</if>
            <if test="meetingTime != null">meeting_time = #{meetingTime},</if>
            <if test="location != null">location = #{location},</if>
            <if test="noticeContent != null">notice_content = #{noticeContent},</if>
            <if test="recordFileUrl != null">record_file_url = #{recordFileUrl},</if>
            <if test="resolutionContent != null">resolution_content = #{resolutionContent},</if>
            <if test="status != null">status = #{status},</if>
            <if test="delFlag != null">del_flag = #{delFlag},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where meeting_id = #{meetingId}
    </update>

    <delete id="deleteSysCommitteeMeetingByMeetingId" parameterType="Long">
        delete from sys_committee_meeting where meeting_id = #{meetingId}
    </delete>

    <delete id="deleteSysCommitteeMeetingByMeetingIds" parameterType="String">
        delete from sys_committee_meeting where meeting_id in 
        <foreach item="meetingId" collection="array" open="(" separator="," close=")">
            #{meetingId}
        </foreach>
    </delete>
</mapper>