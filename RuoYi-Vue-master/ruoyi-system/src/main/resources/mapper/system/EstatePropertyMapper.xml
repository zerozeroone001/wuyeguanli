<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.EstatePropertyMapper">
    
    <resultMap type="EstateProperty" id="EstatePropertyResult">
        <result property="propertyId"    column="property_id"    />
        <result property="communityId"    column="community_id"    />
        <result property="buildingName"    column="building_name"    />
        <result property="unitName"    column="unit_name"    />
        <result property="roomNumber"    column="room_number"    />
        <result property="floor"    column="floor"    />
        <result property="area"    column="area"    />
        <result property="status"    column="status"    />
        <result property="delFlag"    column="del_flag"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectEstatePropertyVo">
        select property_id, community_id, building_name, unit_name, room_number, floor, area, status, del_flag, create_by, create_time, update_by, update_time, remark from estate_property
    </sql>

    <select id="selectEstatePropertyList" parameterType="EstateProperty" resultMap="EstatePropertyResult">
        <include refid="selectEstatePropertyVo"/>
        <where>  
            <if test="communityId != null "> and community_id = #{communityId}</if>
            <if test="buildingName != null  and buildingName != ''"> and building_name like concat('%', #{buildingName}, '%')</if>
            <if test="unitName != null  and unitName != ''"> and unit_name like concat('%', #{unitName}, '%')</if>
            <if test="roomNumber != null  and roomNumber != ''"> and room_number = #{roomNumber}</if>
            <if test="floor != null "> and floor = #{floor}</if>
            <if test="area != null "> and area = #{area}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
        </where>
    </select>
    
    <select id="selectEstatePropertyByPropertyId" parameterType="Long" resultMap="EstatePropertyResult">
        <include refid="selectEstatePropertyVo"/>
        where property_id = #{propertyId}
    </select>

    <insert id="insertEstateProperty" parameterType="EstateProperty" useGeneratedKeys="true" keyProperty="propertyId">
        insert into estate_property
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="communityId != null">community_id,</if>
            <if test="buildingName != null and buildingName != ''">building_name,</if>
            <if test="unitName != null">unit_name,</if>
            <if test="roomNumber != null and roomNumber != ''">room_number,</if>
            <if test="floor != null">floor,</if>
            <if test="area != null">area,</if>
            <if test="status != null and status != ''">status,</if>
            <if test="delFlag != null">del_flag,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="communityId != null">#{communityId},</if>
            <if test="buildingName != null and buildingName != ''">#{buildingName},</if>
            <if test="unitName != null">#{unitName},</if>
            <if test="roomNumber != null and roomNumber != ''">#{roomNumber},</if>
            <if test="floor != null">#{floor},</if>
            <if test="area != null">#{area},</if>
            <if test="status != null and status != ''">#{status},</if>
            <if test="delFlag != null">#{delFlag},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateEstateProperty" parameterType="EstateProperty">
        update estate_property
        <trim prefix="SET" suffixOverrides=",">
            <if test="communityId != null">community_id = #{communityId},</if>
            <if test="buildingName != null and buildingName != ''">building_name = #{buildingName},</if>
            <if test="unitName != null">unit_name = #{unitName},</if>
            <if test="roomNumber != null and roomNumber != ''">room_number = #{roomNumber},</if>
            <if test="floor != null">floor = #{floor},</if>
            <if test="area != null">area = #{area},</if>
            <if test="status != null and status != ''">status = #{status},</if>
            <if test="delFlag != null">del_flag = #{delFlag},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where property_id = #{propertyId}
    </update>

    <delete id="deleteEstatePropertyByPropertyId" parameterType="Long">
        delete from estate_property where property_id = #{propertyId}
    </delete>

    <delete id="deleteEstatePropertyByPropertyIds" parameterType="String">
        delete from estate_property where property_id in 
        <foreach item="propertyId" collection="array" open="(" separator="," close=")">
            #{propertyId}
        </foreach>
    </delete>
</mapper>