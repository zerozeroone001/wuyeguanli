<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.SysPropertyRegulationMapper">
    
    <resultMap type="SysPropertyRegulation" id="SysPropertyRegulationResult">
        <result property="regulationId"    column="regulation_id"    />
        <result property="regulationName"    column="regulation_name"    />
        <result property="categoryId"    column="category_id"    />
        <result property="regulationType"    column="regulation_type"    />
        <result property="summary"    column="summary"    />
        <result property="publishDept"    column="publish_dept"    />
        <result property="isImportant"    column="is_important"    />
        <result property="chapters"    column="chapters"    />
        <result property="revisionHistory"    column="revision_history"    />
        <result property="filePath"    column="file_path"    />
        <result property="effectiveDate"    column="effective_date"    />
        <result property="version"    column="version"    />
        <result property="status"    column="status"    />
        <result property="viewCount"    column="view_count"    />
        <result property="downloadCount"    column="download_count"    />
        <result property="favoriteCount"    column="favorite_count"    />
        <result property="fileSize"    column="file_size"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectSysPropertyRegulationVo">
        select regulation_id, regulation_name, category_id, regulation_type, summary, publish_dept, is_important, chapters, revision_history, file_path, effective_date, version, status, view_count, download_count, favorite_count, file_size, create_by, create_time, update_by, update_time, remark from sys_property_regulation
    </sql>

    <select id="selectSysPropertyRegulationList" parameterType="SysPropertyRegulation" resultMap="SysPropertyRegulationResult">
        <include refid="selectSysPropertyRegulationVo"/>
        <where>  
            <if test="regulationName != null  and regulationName != ''"> and regulation_name like concat('%', #{regulationName}, '%')</if>
            <if test="categoryId != null "> and category_id = #{categoryId}</if>
            <if test="regulationType != null  and regulationType != ''"> and regulation_type = #{regulationType}</if>
            <if test="publishDept != null  and publishDept != ''"> and publish_dept like concat('%', #{publishDept}, '%')</if>
            <if test="isImportant != null "> and is_important = #{isImportant}</if>
            <if test="effectiveDate != null "> and effective_date = #{effectiveDate}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
        </where>
    </select>
    
    <select id="selectSysPropertyRegulationByRegulationId" parameterType="Long" resultMap="SysPropertyRegulationResult">
        <include refid="selectSysPropertyRegulationVo"/>
        where regulation_id = #{regulationId}
    </select>
        
    <insert id="insertSysPropertyRegulation" parameterType="SysPropertyRegulation" useGeneratedKeys="true" keyProperty="regulationId">
        insert into sys_property_regulation
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="regulationName != null and regulationName != ''">regulation_name,</if>
            <if test="categoryId != null">category_id,</if>
            <if test="regulationType != null and regulationType != ''">regulation_type,</if>
            <if test="summary != null">summary,</if>
            <if test="publishDept != null and publishDept != ''">publish_dept,</if>
            <if test="isImportant != null">is_important,</if>
            <if test="chapters != null">chapters,</if>
            <if test="revisionHistory != null">revision_history,</if>
            <if test="filePath != null">file_path,</if>
            <if test="effectiveDate != null">effective_date,</if>
            <if test="version != null">version,</if>
            <if test="status != null and status != ''">status,</if>
            <if test="viewCount != null">view_count,</if>
            <if test="downloadCount != null">download_count,</if>
            <if test="favoriteCount != null">favorite_count,</if>
            <if test="fileSize != null">file_size,</if>
            <if test="createBy != null and createBy != ''">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null and updateBy != ''">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="regulationName != null and regulationName != ''">#{regulationName},</if>
            <if test="categoryId != null">#{categoryId},</if>
            <if test="regulationType != null and regulationType != ''">#{regulationType},</if>
            <if test="summary != null">#{summary},</if>
            <if test="publishDept != null and publishDept != ''">#{publishDept},</if>
            <if test="isImportant != null">#{isImportant},</if>
            <if test="chapters != null">#{chapters},</if>
            <if test="revisionHistory != null">#{revisionHistory},</if>
            <if test="filePath != null">#{filePath},</if>
            <if test="effectiveDate != null">#{effectiveDate},</if>
            <if test="version != null">#{version},</if>
            <if test="status != null and status != ''">#{status},</if>
            <if test="viewCount != null">#{viewCount},</if>
            <if test="downloadCount != null">#{downloadCount},</if>
            <if test="favoriteCount != null">#{favoriteCount},</if>
            <if test="fileSize != null">#{fileSize},</if>
            <if test="createBy != null and createBy != ''">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null and updateBy != ''">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateSysPropertyRegulation" parameterType="SysPropertyRegulation">
        update sys_property_regulation
        <trim prefix="SET" suffixOverrides=",">
            <if test="regulationName != null and regulationName != ''">regulation_name = #{regulationName},</if>
            <if test="categoryId != null">category_id = #{categoryId},</if>
            <if test="regulationType != null and regulationType != ''">regulation_type = #{regulationType},</if>
            <if test="summary != null">summary = #{summary},</if>
            <if test="publishDept != null and publishDept != ''">publish_dept = #{publishDept},</if>
            <if test="isImportant != null">is_important = #{isImportant},</if>
            <if test="chapters != null">chapters = #{chapters},</if>
            <if test="revisionHistory != null">revision_history = #{revisionHistory},</if>
            <if test="filePath != null">file_path = #{filePath},</if>
            <if test="effectiveDate != null">effective_date = #{effectiveDate},</if>
            <if test="version != null">version = #{version},</if>
            <if test="status != null and status != ''">status = #{status},</if>
            <if test="viewCount != null">view_count = #{viewCount},</if>
            <if test="downloadCount != null">download_count = #{downloadCount},</if>
            <if test="favoriteCount != null">favorite_count = #{favoriteCount},</if>
            <if test="fileSize != null">file_size = #{fileSize},</if>
            <if test="createBy != null and createBy != ''">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null and updateBy != ''">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where regulation_id = #{regulationId}
    </update>

    <delete id="deleteSysPropertyRegulationByRegulationId" parameterType="Long">
        delete from sys_property_regulation where regulation_id = #{regulationId}
    </delete>

    <delete id="deleteSysPropertyRegulationByRegulationIds" parameterType="String">
        delete from sys_property_regulation where regulation_id in 
        <foreach item="regulationId" collection="array" open="(" separator="," close=")">
            #{regulationId}
        </foreach>
    </delete>
</mapper>