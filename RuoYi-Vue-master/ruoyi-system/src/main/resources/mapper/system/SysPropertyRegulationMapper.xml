<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.SysPropertyRegulationMapper">
    
    <resultMap type="SysPropertyRegulation" id="SysPropertyRegulationResult">
        <result property="regulationId"    column="regulation_id"    />
        <result property="regulationName"    column="regulation_name"    />
        <result property="regulationType"    column="regulation_type"    />
        <result property="regulationContent"    column="regulation_content"    />
        <result property="filePath"    column="file_path"    />
        <result property="effectiveDate"    column="effective_date"    />
        <result property="version"    column="version"    />
        <result property="status"    column="status"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectSysPropertyRegulationVo">
        select regulation_id, regulation_name, regulation_type, regulation_content, file_path, effective_date, version, status, create_by, create_time, update_by, update_time, remark from sys_property_regulation
    </sql>

    <select id="selectSysPropertyRegulationList" parameterType="SysPropertyRegulation" resultMap="SysPropertyRegulationResult">
        <include refid="selectSysPropertyRegulationVo"/>
        <where>  
            <if test="regulationName != null  and regulationName != ''"> and regulation_name like concat('%', #{regulationName}, '%')</if>
            <if test="regulationType != null  and regulationType != ''"> and regulation_type = #{regulationType}</if>
            <if test="regulationContent != null  and regulationContent != ''"> and regulation_content = #{regulationContent}</if>
            <if test="filePath != null  and filePath != ''"> and file_path = #{filePath}</if>
            <if test="effectiveDate != null "> and effective_date = #{effectiveDate}</if>
            <if test="version != null  and version != ''"> and version = #{version}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
        </where>
    </select>
    
    <select id="selectSysPropertyRegulationByRegulationId" parameterType="Long" resultMap="SysPropertyRegulationResult">
        <include refid="selectSysPropertyRegulationVo"/>
        where regulation_id = #{regulationId}
    </select>

    <insert id="insertSysPropertyRegulation" parameterType="SysPropertyRegulation" useGeneratedKeys="true" keyProperty="regulationId">
        insert into sys_property_regulation
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="regulationName != null and regulationName != ''">regulation_name,</if>
            <if test="regulationType != null and regulationType != ''">regulation_type,</if>
            <if test="regulationContent != null">regulation_content,</if>
            <if test="filePath != null">file_path,</if>
            <if test="effectiveDate != null">effective_date,</if>
            <if test="version != null">version,</if>
            <if test="status != null">status,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="regulationName != null and regulationName != ''">#{regulationName},</if>
            <if test="regulationType != null and regulationType != ''">#{regulationType},</if>
            <if test="regulationContent != null">#{regulationContent},</if>
            <if test="filePath != null">#{filePath},</if>
            <if test="effectiveDate != null">#{effectiveDate},</if>
            <if test="version != null">#{version},</if>
            <if test="status != null">#{status},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateSysPropertyRegulation" parameterType="SysPropertyRegulation">
        update sys_property_regulation
        <trim prefix="SET" suffixOverrides=",">
            <if test="regulationName != null and regulationName != ''">regulation_name = #{regulationName},</if>
            <if test="regulationType != null and regulationType != ''">regulation_type = #{regulationType},</if>
            <if test="regulationContent != null">regulation_content = #{regulationContent},</if>
            <if test="filePath != null">file_path = #{filePath},</if>
            <if test="effectiveDate != null">effective_date = #{effectiveDate},</if>
            <if test="version != null">version = #{version},</if>
            <if test="status != null">status = #{status},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where regulation_id = #{regulationId}
    </update>

    <delete id="deleteSysPropertyRegulationByRegulationId" parameterType="Long">
        delete from sys_property_regulation where regulation_id = #{regulationId}
    </delete>

    <delete id="deleteSysPropertyRegulationByRegulationIds" parameterType="String">
        delete from sys_property_regulation where regulation_id in 
        <foreach item="regulationId" collection="array" open="(" separator="," close=")">
            #{regulationId}
        </foreach>
    </delete>
</mapper>