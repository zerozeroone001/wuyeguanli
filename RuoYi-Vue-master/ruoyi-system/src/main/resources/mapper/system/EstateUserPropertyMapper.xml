<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.EstateUserPropertyMapper">
    
    <resultMap type="EstateUserProperty" id="EstateUserPropertyResult">
        <result property="associationId"    column="association_id"    />
        <result property="userId"    column="user_id"    />
        <result property="propertyId"    column="property_id"    />
        <result property="communityId"    column="community_id"    />
        <result property="userType"    column="user_type"    />
        <result property="status"    column="status"    />
        <result property="delFlag"    column="del_flag"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectEstateUserPropertyVo">
        select association_id, user_id, property_id, community_id, user_type, status, del_flag, create_by, create_time, update_by, update_time, remark from estate_user_property
    </sql>

    <select id="selectEstateUserPropertyList" parameterType="EstateUserProperty" resultMap="EstateUserPropertyResult">
        <include refid="selectEstateUserPropertyVo"/>
        <where>  
            <if test="userId != null "> and user_id = #{userId}</if>
            <if test="propertyId != null "> and property_id = #{propertyId}</if>
            <if test="communityId != null "> and community_id = #{communityId}</if>
            <if test="userType != null  and userType != ''"> and user_type = #{userType}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
        </where>
    </select>
    
    <select id="selectEstateUserPropertyByAssociationId" parameterType="Long" resultMap="EstateUserPropertyResult">
        <include refid="selectEstateUserPropertyVo"/>
        where association_id = #{associationId}
    </select>

    <insert id="insertEstateUserProperty" parameterType="EstateUserProperty" useGeneratedKeys="true" keyProperty="associationId">
        insert into estate_user_property
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="userId != null">user_id,</if>
            <if test="propertyId != null">property_id,</if>
            <if test="communityId != null">community_id,</if>
            <if test="userType != null">user_type,</if>
            <if test="status != null">status,</if>
            <if test="delFlag != null">del_flag,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="userId != null">#{userId},</if>
            <if test="propertyId != null">#{propertyId},</if>
            <if test="communityId != null">#{communityId},</if>
            <if test="userType != null">#{userType},</if>
            <if test="status != null">#{status},</if>
            <if test="delFlag != null">#{delFlag},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateEstateUserProperty" parameterType="EstateUserProperty">
        update estate_user_property
        <trim prefix="SET" suffixOverrides=",">
            <if test="userId != null">user_id = #{userId},</if>
            <if test="propertyId != null">property_id = #{propertyId},</if>
            <if test="communityId != null">community_id = #{communityId},</if>
            <if test="userType != null">user_type = #{userType},</if>
            <if test="status != null">status = #{status},</if>
            <if test="delFlag != null">del_flag = #{delFlag},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where association_id = #{associationId}
    </update>

    <delete id="deleteEstateUserPropertyByAssociationId" parameterType="Long">
        delete from estate_user_property where association_id = #{associationId}
    </delete>

    <delete id="deleteEstateUserPropertyByAssociationIds" parameterType="String">
        delete from estate_user_property where association_id in 
        <foreach item="associationId" collection="array" open="(" separator="," close=")">
            #{associationId}
        </foreach>
    </delete>
</mapper>