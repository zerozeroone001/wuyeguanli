<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.SysContractFileMapper">

    <insert id="batchInsert">
        insert into sys_contract_file(contract_id, file_id) values
        <foreach item="item" index="index" collection="list" separator=",">
            (#{item.contractId}, #{item.fileId})
        </foreach>
    </insert>

    <delete id="deleteByContractId" parameterType="Long">
        delete from sys_contract_file where contract_id = #{contractId}
    </delete>

</mapper>